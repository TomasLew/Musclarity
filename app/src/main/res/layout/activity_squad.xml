        />

    <com.example.musclarity.GradientProgressBar
        android:id="@+id/energy_DFC2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="65dp"
        android:layout_height="10dp"
        app:layout_constraintTop_toBottomOf="@id/DFC2"
        app:layout_constraintStart_toStartOf="@id/DFC2"
        app:layout_constraintEnd_toEndOf="@id/DFC2"
        />

    <ImageView
        android:id="@+id/DFD"
